# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET     /                           controllers.HomeController.index
GET     /init                       controllers.HomeController.init
GET     /store                      controllers.HomeController.store
GET     /cart                       controllers.HomeController.cart
GET     /confirm                    controllers.HomeController.confirm
GET     /checkout                   controllers.HomeController.checkout

POST    /add                        controllers.HomeController.addToCart
POST    /update                     controllers.HomeController.update


GET     /assets/*file               controllers.Assets.at(path="/public", file)
